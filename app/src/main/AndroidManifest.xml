<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kokist.android.webtest" >


    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JobActivity"
            android:label="@string/zhaopin"/>
        <activity android:name=".LoginActivity"
            android:label="@string/loign"/>
        <activity android:name=".RegActivity"
            android:label="@string/reg"/>
        <activity android:name=".ValidateActivity"
            android:label="@string/please_input_valicode"
            />
    </application>

</manifest>
